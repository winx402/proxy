<?xml version="1.0" encoding="UTF-8"?>
<mapers>
    <target>
        <web>http://www.xicidaili.com</web>
        <entrance>http://www.xicidaili.com</entrance>
        <shouldvisit>^http://www.xicidaili.com/(nn|nt|wn|wt)/?\d{0,}$</shouldvisit>
        <lineType>table</lineType>
        <ip>tableIpPattern</ip>
        <port>tablePortPattern</port>
        <type>tableTypePattern</type>
    </target>

    <target>
        <web>http://www.youdaili.net</web>
        <entrance>http://www.youdaili.net/daili/(http|guonei|qq|socks)/?(list_\d{1,}.html)?</entrance>
        <shouldvisit>^http://www.youdaili.net/daili/(http|guonei|qq|socks)/\d{1,}.html$</shouldvisit>
        <lineType>p</lineType>
        <ip>pIpPattern</ip>
        <port>pPortPattern</port>
        <type>pTypePattern</type>
    </target>
</mapers>